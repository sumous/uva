#include<stdio.h>
double find_plo(char ch)//找出对应的前缀 
{
	if(ch == 'm') return 0.001;
	
	if(ch == 'k') return 1000;
	
	if(ch == 'M') return 1000000;
	
	return 1;
}

int main()
{
	freopen("data.txt", "r", stdin);
	double P, U, I;
	char ch;
	int flag, n, i;
	scanf("%d", &n) 
	
	for( i = 1;i <= n;i ++)
	{
		P = 0.0;U = 0.0;I = 0.0;
		getchar();
		while(1)
		{
			ch = getchar();
			if(ch == 'P')
			{
				getchar();
				scanf("%f", &P);
				ch = getchar();
				P *= find_plo(ch);			
				flag++;
			}
			
			if(ch == 'U')
			{
				getchar();
				scanf("%f", &I);
				ch = getchar();
				U *= find_plo(ch);
				flag++;
			}
			
			if(ch == 'I')
			{
				getchar();
				scanf("%f", &I);
				ch = getchar();
				I *= find_plo(ch);
				flag++;
			}
			
			if(flag == 2)
			{
				gets();
				break;
			}
			
			
		}
		printf("Problem #%d\n", i);
		if(I == 0.0)
		{
			printf("I=%.2fA\n\n", P/U);
		}
		else if(U == 0.0)
		{
			printf("U=%.2fV\n\n", P/I);
		}
		else printf("P=%.2fW\n\n", U*I);
	}
	return 0;
}
